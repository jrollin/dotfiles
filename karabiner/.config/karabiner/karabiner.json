{
    "profiles": [
        {
            "complex_modifications": {
                "parameters": {
                    "basic.simultaneous_threshold_milliseconds": 500,
                    "basic.to_delayed_action_delay_milliseconds": 100,
                    "basic.to_if_held_down_threshold_milliseconds": 100
                },
                "rules": [
                    {
                        "description": "Launch new Terminal",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "return_or_enter",
                                    "modifiers": { "mandatory": ["command"] }
                                },
                                "to": [{ "shell_command": "open -na Ghostty" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Shift + Esc to #",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "escape",
                                    "modifiers": { "mandatory": ["shift"] }
                                },
                                "to": [
                                    {
                                        "key_code": "grave_accent_and_tilde",
                                        "modifiers": ["shift"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Command + Shift + Esc to @",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "escape",
                                    "modifiers": { "mandatory": ["command", "shift"] }
                                },
                                "to": [{ "key_code": "grave_accent_and_tilde" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Post Esc if Caps is tapped, Control if held.",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "left_control",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    {
                                        "key_code": "left_control",
                                        "lazy": true
                                    }
                                ],
                                "to_if_alone": [{ "key_code": "escape" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Map Control_Shift + C/V to copy/paste in term (ala Linux)",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.apple\\.Terminal$",
                                            "^com\\.googlecode\\.iterm2$",
                                            "^co\\.zeit\\.hyperterm$",
                                            "^co\\.zeit\\.hyper$",
                                            "^io\\.alacritty$",
                                            "^org\\.alacritty$",
                                            "^net\\.kovidgoyal\\.kitty$",
                                            "^com\\.mitchellh\\.ghostty$"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": {
                                    "key_code": "c",
                                    "modifiers": {
                                        "mandatory": ["control", "left_shift"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "c",
                                        "modifiers": ["command"]
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
